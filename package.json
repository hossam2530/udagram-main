{
    "scripts": {
        "frontend:install": "cd udagram-main/udagram-frontend && npm install -f",
        "frontend:start": "cd udagram-main/udagram-frontend && npm run start",
        "frontend:build": "cd udagram-main/udagram-frontend && npm run build",
        "frontend:test": "cd udagram-main/udagram-frontend && npm run test",
        "frontend:e2e": "cd udagram-main/udagram-frontend && npm run e2e",
        "frontend:lint": "cd udagram-main/udagram-frontend && npm run lint",
        "frontend:deploy": "cd udagram-main/udagram-frontend && npm run deploy",
        "api:install": "cd udagram-main/udagram-api && npm install .",
        "api:build": "cd udagram-main/udagram-api && npm run build",
        "api:start": "cd udagram-main/udagram-api && npm run dev",
        "api:deploy": "cd udagram-main/udagram-api && npm run deploy",
        "deploy": "npm run api:deploy && npm run frontend:deploy",
        "set:env":"cd udagram-main && chmod +x bin/initEnv.sh && bin/initEnv.sh"
    }
}
